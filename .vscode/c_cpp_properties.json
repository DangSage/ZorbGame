{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include",
                "/usr/local/include"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64"
        },
        {
            "name": "Windows",
            "includePath": [
                "/Program Files (x86)/Windows Kits/10/Include",
                "${workspaceFolder}\\include",
                "${workspaceFolder}\\bin",
                "${workspaceFolder}/**"
            ],
            "defines": [],
            "compilerPath": "/msys64/mingw64/bin/g++.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "${default}",
            "mergeConfigurations": false,
            "browse": {
                "path": [
                    "/Program Files (x86)/Windows Kits/10/Include",
                    "${workspaceFolder}\\include",
                    "${workspaceFolder}\\bin",
                    "${workspaceFolder}/**"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}